<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="lbTeam"/>
			<g:ListBox ui:field="lstTeams" width="90px"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="lbPlayer"/>
			<g:ListBox ui:field="lstPlayers" width="90px"/>
		</g:HorizontalPanel>		
		<g:HorizontalPanel>
			<g:Button ui:field="btnOk"/>
			<g:Button ui:field="btnCancel"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 