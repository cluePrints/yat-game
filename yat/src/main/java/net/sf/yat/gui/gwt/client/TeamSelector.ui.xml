<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label text="Team:"/>
			<g:ListBox ui:field="lstTeams" width="90px"/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Player:"/>
			<g:ListBox ui:field="lstPlayers" width="90px"/>
		</g:HorizontalPanel>		
		<g:HorizontalPanel>
			<g:Button ui:field="btnOk" text="OK"/>
			<g:Button ui:field="btnCancel" text="Cancel"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 