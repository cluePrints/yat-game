<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.4//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name="com.google.gwt.user.User" />

	<!--<inherits name='com.google.gwt.user.theme.standard.Standard'/>-->
	<!--<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>-->
    <!--<inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	
	<inherits name="net.sf.yat.Domain" />		
	
	<source path="client" />
	<entry-point class="net.sf.yat.gui.gwt.client.Entry"></entry-point>
	
	<!-- known: ie6, gecko1_8, safari, remove this prop to compile for all browser supported-->
	<set-property name="user.agent" value="gecko1_8"/> 
	
	<inherits name="com.google.gwt.i18n.I18N"/>
	<extend-property name="locale" values="en"/>
	<extend-property name="locale" values="ru"/>	
</module>